    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.0"
  }
